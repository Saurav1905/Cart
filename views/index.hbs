<h1>{{title}}</h1>
{{# each products }}
<div class="row wish">
  <div class="col-md-12 wishlist">
    <div class="thumbnail"> <span class="image-container"><img src="" alt=""></span>
      <div class="caption">
        <h3>{{ this.title }}</h3>
        <p class="description">{{{this.description}}}</p>
        <div class="clearfix">
          <div class="price pull-left"><i class="fa fa-tag" aria-hidden="true"></i> Rs:{{ this.price }}/- </div>
          <a href="/add/{{ this.id }}" class="add-to-cart btn btn-default pull-right" role="button"><i
              class="fa fa-shopping-cart" aria-hidden="true"></i> Add to cart</a>
        </div>
      </div>
    </div>
  </div>
</div>

{{/each}}
